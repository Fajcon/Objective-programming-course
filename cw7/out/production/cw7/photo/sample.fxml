      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="600.0" prefWidth="1000.0">
         <children>
            <ListView fx:id="listOfFilesView" layoutX="30.0" layoutY="36.0" prefHeight="510.0" prefWidth="309.0" />
            <Button fx:id="viewFotoButton" layoutX="359.0" layoutY="36.0" mnemonicParsing="false" onAction="#viewFoto" text="View foto" />
            <ImageView fx:id="fotoView" fitHeight="469.0" fitWidth="607.0" layoutX="359.0" layoutY="79.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </Pane>
   </children>
